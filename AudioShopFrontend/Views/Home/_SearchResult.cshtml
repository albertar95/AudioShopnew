@model List<AudioShopFrontend.DTO.ProductDTO>

@foreach (var item in Model)
{
    <div class="" style="text-align:right;border:1px solid #d6cece;border-radius:1px;">
    <p style="margin:10px;direction:rtl"><a href="@Url.Action("Product",new { NidProduct = item.NidProduct})" style="text-align:right;">@item.ProductName</a></p></div>
}
